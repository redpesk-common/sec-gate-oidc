    "version": "0.2.0",
    "configurations": [
        {

          // afb-binder --name=afb-oidc --config=${workspaceFolder}/conf.d/project/etc/oidc-basic.json
          "name": "oidc-dex",
          "type": "cppdbg",
          "request": "launch",
          "program": "/usr/local/bin/afb-binder",
          "args": [
            "--name=afb-oidc",
            "--config=${workspaceFolder}/conf.d/project/etc/oidc-dex.json",
            "--rootdir=${workspaceFolder}/conf.d/project/htdocs",
            "--roothttp=.",
            "--verbose","--verbose","--verbose",
            "--trap-faults=no"
          ],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}/build",
          "environment": [{"name":"LD_LIBRARY_PATH", "value":"package/lib:package/lib/plugins:/usr/local/lib64:/usr/local/lib:/usr/local/fedid-binding/lib"}],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            }
          ],
          "miDebuggerPath": "/usr/bin/gdb"
        },
        {
          // afb-binder --name=afb-oidc --config=${workspaceFolder}/conf.d/project/etc/oidc-basic.json
          "name": "sync-group-6",
